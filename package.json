{
  "name": "website-v2",
  "version": "2.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist",
    "build:prod": "ng build --configuration=production",
